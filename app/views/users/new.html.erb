<h1>Sign up</h1>
<%= form_for @user do |f| %>
<p>
  <%= f.label :username %>
  <%= f.text_field :username%>
</p>
<p>
  <%= f.label :password %>
  <%= f.password_field :password%>
</p>
<p>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation%>
</p>
<p><%= f.submit %>
</p>
<% end %>


<h1>Log in</h1>
<%= form_tag sessions_path do %>
<p>
  <%= label_tag :username %>
  <%= text_field_tag :username%>
</p>
<p>
  <%= label_tag :password %>
  <%= password_field_tag :password%>
</p>
<p>
  <%= submit_tag "Log in" %>
</p>
<% end %>
